{
  "name": "depresearch",
  "version": "0.0.1",
  "description": "Research any open-source repo without leaving your project.",
  "type": "module",
  "license": "MIT",
  "bin": {
    "depresearch": "./dist/index.js",
    "dpr": "./dist/index.js"
  },
  "files": [
    "dist",
    "!dist/**/*.map"
  ],
  "keywords": [
    "cli",
    "research",
    "open-source",
    "source-code",
    "ai",
    "mastra",
    "typescript",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kristianvtr/depresearch"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "citty": "^0.2.1",
    "consola": "^3.4.2"
  },
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "^5.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
