{
  "name": "depresearch",
  "version": "0.0.1",
  "description": "Deep research for your dependencies. Understand any JS/TS library by analyzing its source code.",
  "type": "module",
  "license": "MIT",
  "bin": {
    "depresearch": "./dist/index.js",
    "dpr": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "research",
    "dependencies",
    "source-code",
    "ai",
    "mastra",
    "typescript",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kristianvtr/depresearch"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@depresearch/shared": "workspace:*",
    "@mastra/core": "latest",
    "@mastra/libsql": "^1.4.0",
    "@mastra/loggers": "^1.0.1",
    "@mastra/memory": "^1.3.0",
    "citty": "^0.2.1",
    "consola": "^3.4.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "^5.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
